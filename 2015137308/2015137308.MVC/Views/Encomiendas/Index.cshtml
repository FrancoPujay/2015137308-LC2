@model IEnumerable<_2015137308.Entities.Entities.Encomienda>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Bus.Placa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LugarViaje.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Beneficiario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Peso)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Bus.Placa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LugarViaje.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Beneficiario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Peso)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ServicioId }) |
            @Html.ActionLink("Details", "Details", new { id=item.ServicioId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ServicioId })
        </td>
    </tr>
}

</table>
